				System.out.print(' ');
			} 
			for(int j = 0; j < 2*i + 1; j++) {
				System.out.print(val);
				if(j < i) {
					val++;
				} else  {
					
					val--;
					
				}
			} for(int j = 0; j < n-i-1; j++) {
				System.out.print(' ');
				
			}
		
			System.out.print('\n');
		}
	}
