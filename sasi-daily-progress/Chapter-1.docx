package p1;

public class Patterns {
	
	public static void pattern1(int n, int m) {
		for(int i = 0; i < n; i++) {
			System.out.print('\n');
			for(int j = 0; j < m; j++) {
				
				System.out.print("*");
			}
		}
	}
	
	public static void pattern2(int m, int n) {
		for(int i = 0; i < n; i++) {
			for(int j = i ; j < m; j++) {
				//System.out.print("\n");
				System.out.print("*");
			}
			
			System.out.print("\n");
		}
	}
	
	public static void pattern3(int m, int n) {
		for(int i = n - 1; i >= 0; i--) {
			
			for(int j = m - 1; j > i - 1; j--) {
				System.out.print("*");
			} 
			System.out.print("\n");
			
		}
	}
	
	public static void pattern4(int m, int n) {
