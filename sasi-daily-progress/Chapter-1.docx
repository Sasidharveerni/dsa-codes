	public static void pattern17(int n) {
		char val = 65;
		// same like the pattern 7 for n = 5 -> 1, 3, 5, 7, 9 -> 2*(n-1) + 1 -> divide by two since two spaces needed 
		// n - 1
		for(int i = 0; i < n; i++) {
			val = 65;
