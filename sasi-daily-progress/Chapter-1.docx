	public static void pattern10(int n) {
		// This we don't need spaces we just need to use new lines properly
		// we need to print 
		for(int i = 0; i < n; i++) {
			for(int j = 0; j <= i; j++) {
				System.out.print('*');
			}
			System.out.print('\n');
		}
		for(int i = n-2; i >= 0; i--) {
			for(int j = 0; j < i; j++) {
				System.out.print('*');
			}
			System.out.print('\n');
		}
	}
	
	public static void pattern11(int n) {
		for(int i = 0; i < n; i++) {
			if(i % 2 == 0) {
				
				for (int j = 0; j <= i; j++) {
				if(j % 2 == 0) {
					System.out.print('1');
					
				} else {
					System.out.print('0');
				}
				}
			} 
			else {
				for (int j = 0; j <= i; j++) {
					if(j % 2 == 0) {
						
						System.out.print('0');
					} else {
						System.out.print('1');
					}
				}
			}
			System.out.print('\n');
		}
	}
	
